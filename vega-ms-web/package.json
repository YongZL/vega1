{
  "name": "vega",
  "version": "4.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8096 IS_GET_VERSION=true  umi build",
    "build:rs": "cross-env NODE_OPTIONS=--max-old-space-size=8096 IS_GET_VERSION=true WEB_PLATFORM=RS umi build",
    "build:prod": "cross-env WEB_PLATFORM=MS BASE_ENV=/hospital/vegabase/ PATH_ENV=/hospital/vegabaseresource/ SERVER_ENV=/hospital/vegabaseserver OVERVIEW_ENV=/hospital/vega-base-overview NODE_OPTIONS=--max-old-space-size=8096 umi build",
    "build:ds": "cross-env NODE_OPTIONS=--max-old-space-size=8096 IS_GET_VERSION=true WEB_PLATFORM=DS umi build",
    "build:dev": "cross-env NODE_OPTIONS=--max-old-space-size=8096 IS_GET_VERSION=true IS_DEV_ENV=true  umi build",
    "build:rs-dev": "cross-env NODE_OPTIONS=--max-old-space-size=8096 IS_GET_VERSION=true IS_DEV_ENV=true WEB_PLATFORM=RS umi build",
    "build:ds-dev": "cross-env NODE_OPTIONS=--max-old-space-size=8096 IS_GET_VERSION=true IS_DEV_ENV=true WEB_PLATFORM=DS umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "local": "cross-env API_NO_LOOP=true umi dev",
    "local:rs": "cross-env API_NO_LOOP=true WEB_PLATFORM=RS umi dev",
    "local:ds": "cross-env API_NO_LOOP=true WEB_PLATFORM=DS umi dev",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:rs": "cross-env WEB_PLATFORM=RS umi dev",
    "start:ds": "cross-env WEB_PLATFORM=DS umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none BASE_ENV=/hospital/gs/ PATH_ENV=/hospital/gsresource/ SERVER_ENV=/hospital/gsserver OVERVIEW_ENV=/hospital/gs-overview umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "report": "nyc report"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^1.0.2",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-form": "1.61.0",
    "@ant-design/pro-layout": "^6.35.0",
    "@ant-design/pro-table": "^2.71.0",
    "@ant-design/pro-provider": "1.6.2",
    "@types/echarts": "^4.6.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/qrcode.react": "^1.0.1",
    "@umijs/route-utils": "^2.0.0",
    "antd": "^4.19.0",
    "classnames": "^2.3.0",
    "dva": "^2.4.0",
    "echarts": "^4.8.0",
    "echarts-for-react": "^2.0.16",
    "git-revision-webpack-plugin": "^3.0.6",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.29.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.5.10",
    "qrcode.react": "^1.0.0",
    "qs": "^6.9.0",
    "react": "^17.0.0",
    "react-barcode": "^1.4.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.2.0",
    "react-pdf-js": "^5.1.0",
    "react-resizable": "^3.0.4",
    "react-router": "^4.3.1",
    "umi": "^3.5.0",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@playwright/test": "^1.17.0",
    "@types/babel-types": "^7.0.8",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/istanbul-lib-coverage": "^2.0.3",
    "@types/istanbul-lib-instrument": "^1.7.4",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/source-map": "^0.5.7",
    "@umijs/fabric": "^2.8.0",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-dumi": "^1.1.0",
    "@umijs/preset-react": "^2.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "istanbul-coverage-display": "^3.0.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "pro-download": "1.0.1",
    "puppeteer-core": "^2.1.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.5.0",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
